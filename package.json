{
    "name": "dpo-cook",
    "version": "1.0.0",
    "description": "Smithsonian DPO Cook - 3D Model/Geometry/Texture Processing Server",
    "scripts": {
        "start": "npm run server",
        "start-service": "node server/scripts/start-service.js",
        "stop-service": "node server/scripts/stop-service.js",
        "server": "nodemon server/bin/index.js",
        "watch": "concurrently \"cd source/client && webpack --watch\" \"tsc -b source/server -w\" \"nodemon server/bin/index.js\"",
        "build": "concurrently \"npm run build-server\" \"npm run build-client\"",
        "build-client": "cd source/client && webpack && cd ../..",
        "build-server": "tsc -b source/server",
        "build-libs": "tsc -b libs",
        "cook": "node server/bin/cli.js",
        "doc": "typedoc --name \"Smithsonian Cook\" --exclude \"**/node_modules/**\" --ignoreCompilerErrors --mode file --target ES6 --theme minimal --out doc/source/ source/",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "nodemonConfig": {
        "delay": "500",
        "watch": [
            "server/bin",
            "server/bin/app",
            "node_modules/@ff"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Smithsonian/dpo-cook.git"
    },
    "contributors": [
        {
            "name": "Ralph Wiedemeier",
            "url": "https://github.com/framelab"
        }
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/Smithsonian/dpo-cook.git/issues"
    },
    "homepage": "https://github.com/Smithsonian/dpo-cook.git#readme",
    "dependencies": {
        "ajv": "^6.10.0",
        "body-parser": "^1.19.0",
        "clone-deep": "^4.0.1",
        "comment-json": "^1.1.3",
        "cookie-parser": "^1.4.4",
        "dotenv": "^8.0.0",
        "express": "^4.17.1",
        "express-handlebars": "^3.1.0",
        "express-session": "^1.16.1",
        "filenamify": "^4.1.0",
        "highlight.js": "^9.15.8",
        "jsonata": "^1.6.4",
        "markdown-table": "^1.1.3",
        "minimist": "^1.2.0",
        "mkdirp": "^0.5.1",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "node-fetch": "^2.6.0",
        "nodemon": "^1.19.1",
        "os-utils": "0.0.14",
        "react-json-tree": "^0.11.2",
        "reactable": "^1.1.0",
        "rimraf": "^2.6.3",
        "socket.io": "^2.2.0",
        "three": "^0.105.2",
        "webdav": "^2.8.0",
        "webdav-server": "^2.4.6"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/chai": "^4.1.7",
        "@types/express": "^4.16.1",
        "@types/express-session": "^1.15.12",
        "@types/json-schema": "^7.0.3",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.0.4",
        "@types/react": "^16.8.19",
        "@types/reactable": "^0.14.6",
        "@types/socket.io": "^2.1.2",
        "apidoc": "^0.17.7",
        "awesome-typescript-loader": "^5.2.1",
        "chai": "^4.2.0",
        "concurrently": "^4.1.0",
        "css-loader": "^1.0.1",
        "mini-css-extract-plugin": "^0.7.0",
        "mocha": "^6.1.4",
        "node-sass": "^4.12.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "sass-loader": "^7.1.0",
        "source-map-loader": "^0.2.4",
        "typedoc": "^0.14.2",
        "typescript": "3.3.3333",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.2"
    }
}
